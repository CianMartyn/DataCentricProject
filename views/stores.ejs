<body>
    <style>
        table, th, td{
            border: 1px solid black;
        }
    </style>

    <h1>Stores</h1>
<table>
    <tr>
        <th>SID</th>
        <th>Location</th>
        <th>Manager ID</th>
        <th>Action</th>
    </tr>
    <% stores.forEach((s) => { %>
        <tr>
            <td><%= s.sid %></td>
            <td><%= s.location %></td>
            <td><%= s.mgrid %></td>
            <td><a href="/stores/edit/<%= s.sid%>">Update</a></td>
        </tr>
    <% }) %>
</table>
<hr>
<a href="/">Home</a>
</body>